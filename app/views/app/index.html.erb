<div ng-app="app">
<h1>App#index</h1>

  <div ng-controller="exController as ctrl">

    <li ng-repeat="exercise in ctrl.exercises">
      {{ $index + 1}} - {{exercise.name}}
    </li>

    <input ng-model="ctrl.newExercise.name">
    <button ng-click="ctrl.add()">Add Exercise</button>
    
  </div>


</div>
